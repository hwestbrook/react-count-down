{
  "name": "react-count-down",
  "version": "1.0.3",
  "description": "a simple count down clock component",
  "main": "lib/count-down.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenIseki/react-count-down.git"
  },
  "author": "Steven Iseki <stevenisekimartin@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=0.13.3 || ^0.14.7"
  },
  "devDependencies": {
    "express": "^4.7.2",
    "babel-core": "^5.6.7",
    "babel-loader": "^5.1.4",
    "node-libs-browser": "^0.5.2",
    "webpack": "^1.9.12",
    "tape": "^4.0.0"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "chrome/22..latest",
      "firefox/16..latest"
    ]
  },
  "scripts": {
    "test": "tape test/*.js",
